def fill_options():
    for i in range(63):
        offset = i * 5
        options.append([offset + 0, offset + 1, offset + 2, offset + 3, offset + 4])

    next_question = 63 * 5
    options.append([next_question + 0, next_question + 1]) # gender

    next_question = next_question + 2
    options.append([next_question + 0, next_question + 1, next_question + 2]) # family status

    next_question = next_question + 3
    options.append([next_question + 0, next_question + 1, next_question + 2, next_question + 3]) # education

    next_question = next_question + 4
    options.append([next_question + 0, next_question + 1]) # visites turkey

    next_question = next_question + 2
    options.append([next_question + 0, next_question + 1, next_question + 2, next_question + 3, next_question + 4]) # sallary

def loop_main():
    fill_options()

    driver.get(website_path)

    for i in range(400):
        time.sleep(3)
        fill_form(i)

    # driver.quit()

def fill_form(counter):

    #Collect all buttons on page and submit button
    buttons = driver.find_elements_by_xpath("//*[@class='appsMaterialWizToggleRadiogroupOffRadio exportOuterCircle']")
    for i in range(len(options)):
        time.sleep(.2)
        buttons[random.choice(options[i])].click()

    check_boxes = driver.find_elements_by_xpath("//*[@class='quantumWizTogglePapercheckboxInnerBox exportInnerBox']")
    time.sleep(.2)
    random.choice(check_boxes).click()

    natioanlity_text = driver.find_element_by_xpath("//*[@name='entry.66285466']")
    time.sleep(.2)
    natioanlity_text.clear()
    natioanlity_text.send_keys("Българин")

    age_text = driver.find_element_by_xpath("//*[@name='entry.2124538564']")
    time.sleep(.2)
    age_text.clear()
    age_text.send_keys(f"{random.randrange(17,63)}")

    proffesion_text = driver.find_element_by_xpath("//*[@name='entry.1640440109']")
    time.sleep(.2)
    proffesion_text.clear()
    proffesion_text.send_keys(f"{random.choice(preffessions)}")

    religion_text = driver.find_element_by_xpath("//*[@name='entry.517462076']")
    time.sleep(.2)
    religion_text.clear()
    religion_text.send_keys('Християнин')

    motivation_text = driver.find_element_by_xpath("//*[@name='entry.875989653']")
    time.sleep(.2)
    motivation_text.clear()
    motivation_text.send_keys(f"{random.choice(motivation)}")

    factors_text = driver.find_element_by_xpath("//*[@name='entry.1272491568']")
    time.sleep(.2)
    factors_text.clear()
    factors_text.send_keys(f"{random.choice(factors)}")

    #Submit form
    submit = driver.find_element_by_xpath("//*[@class='appsMaterialWizButtonPaperbuttonLabel quantumWizButtonPaperbuttonLabel exportLabel']")
    time.sleep(.2)
    submit.click()

    #Go to previous page, which will be the form
    time.sleep(.2)
    driver.execute_script("window.history.go(-1)")

    print(f"Form #{counter} has been submitted!")

if __name__ == '__main__':
    loop_main()
